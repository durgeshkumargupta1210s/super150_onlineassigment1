import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
        int [] arry = new int [n];
		for(int i=0; i<n;i++){
		    arry[i]=sc.nextInt();
        }
        int max=arry[0];
        for(int i=1; i<arry.length;i++){
            if(max<arry[i]){
                max=arry[i];
            }
        }
        System.out.println(max);

    }
}
