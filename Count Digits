import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int  num1 = sc.nextInt();
        int  num2 = sc.nextInt();
        int count=0;
        while(num1!=0){
            int rem= num1%10;
            if(rem==num2){
                count++;
            }
            num1=num1/10;
        }
        System.out.println(count);



    }
}
