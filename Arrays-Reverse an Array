import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
        int [] arry = new int [n];
		for(int i=0; i<n;i++){
		    arry[i]=sc.nextInt();
        }
        for(int i=n-1; i>=0; i--){
            System.out.println(arry[i]);
        }

    }
}
